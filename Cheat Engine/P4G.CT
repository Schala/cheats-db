					  <ID>253</ID>
					  <Description>"Perform Adding All Items of Specific Type"</Description>
					  <VariableType>Auto Assembler Script</VariableType>
					  <AssemblerScript>[ENABLE]
{$lua}
local ItemTypeToAdd = ReadInteger(getAddress("ItemTypeToAdd"))
local InventoryBaseAddr = getAddress("MainPlayerInventoryPtr")
local ValidItemArrayBasePtr = getAddress("ValidItemArray")
local QtyToAdd = ReadBytes(getAddress("QtyToAdd"))
local CurrentItem = 0

local StartItemType = ItemTypeToAdd * 0x100
local EndItemType = StartItemType + 0xFF

while (CurrentItem &lt; 0x1000) do
	if (CurrentItem &gt;= StartItemType and CurrentItem &lt;= EndItemType) then
	  if (ReadBytes(ValidItemArrayBasePtr + CurrentItem) == 1) then
		WriteBytes(InventoryBaseAddr + CurrentItem,QtyToAdd)
	  end
	end
	CurrentItem = CurrentItem + 1
end

